{
  "suggest.echodocSupport": true,
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "eslint.nodePath": "/usr/local/bin/eslint",
  "tsserver.trace.server": "verbose",
  "tsserver.log": "verbose",
  "tsserver.tsdk": "${workspaceFolder}/.yarn/sdks/typescript/lib",
  "tsserver.useLocalTsdk": true,
  "typescript.showUnused": false,
  "typescript.suggestionActions.enabled": false,
  "diagnostic.errorSign": "e",
  "diagnostic.warningSign": "w",
  "diagnostic.infoSign": "i",
  "diagnostic.hintSign": "h",
  "list.insertMappings": {
    "<C-n>": "normal:j",
    "<C-p>": "normal:k"
  },
  "codeLens.enable": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
  "snippets.ultisnips.directories": [
    "~/.config/nvim/UltiSnips"
  ],

  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        "elm.json"
      ],
      "initializationOptions": {
        "elmPath": "elm",
        "elmFormatPath": "elm-format",
        "elmTestPath": "elm-test",
        "elmAnalyseTrigger": "change"
      }
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp", "--debug"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "cSpell.userWords": [
    "Barchart",
    "Carmi",
    "Corvid",
    "Downvotes",
    "GFPP",
    "GPTAPI",
    "Grpc",
    "Kublinskas",
    "Monoid",
    "OPENAI",
    "Reparenting",
    "Testkits",
    "Tiptap",
    "Upvotes",
    "Vaidas",
    "Velo",
    "alacritty",
    "backticks",
    "browserslist",
    "chatgpt",
    "clsx",
    "codegen",
    "consts",
    "dragonofdeath",
    "editorx",
    "esbuild",
    "evid",
    "fieldsets",
    "ftstystem",
    "ftsystem",
    "giphy",
    "immigrator",
    "nobuild",
    "oembed",
    "platformized",
    "pnpm",
    "prosemirror",
    "protos",
    "remirror",
    "reparent",
    "repluggable",
    "ricos",
    "shellexpand",
    "strikethrough",
    "teamcity",
    "testkit",
    "tsbuildinfo",
    "typecheck",
    "vitest",
    "wixpress",
    "wixui",
    "yoshi"
  ],
  "cSpell.enabledLanguageIds": [
    "vim",
